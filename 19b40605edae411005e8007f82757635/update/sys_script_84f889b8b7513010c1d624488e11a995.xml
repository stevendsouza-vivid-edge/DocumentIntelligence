        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {
    var solutionName = current.getValue("solution_name");

    if (solutionName.includes("docintel_ocr_workflow")) {
        gs.include("DocIntelProcessOcrResult");
        var ocrProcessor = new sn_docintel.DocIntelProcessOcrResult();
        ocrProcessor.process(current);
    } else if (solutionName.includes("docintel_prediction_workflow")) {
        gs.include("DocIntelProcessPredictionResult");
        var predictionProcessor = new sn_docintel.DocIntelProcessPredictionResult();
        predictionProcessor.process(current);
    } else if (solutionName.includes("docintel_training_workflow")) {
        gs.include("DocIntelProcessTrainingResult");
        var trainingProcessor = new sn_docintel.DocIntelProcessTrainingResult();
        trainingProcessor.process(current);
    } else if (solutionName.includes("docintel_pdf_preprocessing_workflow")) {
        gs.include("DocIntelProcessPdfResult");
        var pdfProcessor = new sn_docintel.DocIntelProcessPdfResult();
        pdfProcessor.process(current);
    } else {
        // This would be weird since the condition on the business rule
        // checks if the solution name contains "docintel"
        gs.error("Unknown 'docintel' ML solution: " + solutionName);
    }
})(current, previous);]]></script>
